#include <stdio.h>
#include <stdlib.h>


 unsigned int najwiekszy_dzielnik_wlasciwy(unsigned int x){
 unsigned int i;
 for( i=x;i>0;i++){
    if(x%i==0){

        return i;
    }
 }
 }



int main(){
int n;
scanf("&d",&n);
printf("najwiekszym dzielnikiem %d jest %d",n,najwiekszy_dzielnik_wlasciwy(n));



}
